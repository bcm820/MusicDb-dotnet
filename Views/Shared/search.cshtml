@{

  ViewBag.Title = "Search Results";
  Layout = "_layout.cshtml";

  <div class="col-sm-6 px-3">
    @foreach (var song in ViewBag.Results) {
      var ArtistLink = $"/artists/{song.ArtistUID}";
      <p>
        <a href=@song.Url>@song.Title</a><br/>
        <a href=@ArtistLink>@song.ArtistName</a>
      </p>
    }
  </div>

  <div class="col-sm-3 px-3">&nbsp;</div>

}